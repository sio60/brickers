################################
# MongoDB
################################
PRIMARY_MONGODB_URI=mongodb://localhost:27017/brickers
PRIMARY_MONGODB_DB=brickers
MONGODB_URI=mongodb://localhost:27017/brickers
KIDS_RENDER_DIR=./uploads/kids/out

################################
# JWT
################################
JWT_SECRET=your_jwt_secret_here
JWT_ACCESS_EXPIRATION=3600000
JWT_REFRESH_EXPIRATION=604800000

################################
# OAuth2 - Kakao
################################
LOCAL_BASE_URL=http://localhost:8080/
BASE_URL=https://brickers.shop/
KAKAO_CLIENT_ID=your_kakao_client_id
KAKAO_CLIENT_SECRET=your_kakao_client_secret
KAKAO_REDIRECT_URI=http://localhost:8080/auth/kakao/callback
VITE_KAKAO_REDIRECT_URI=https://brickers.shop/auth/kakao/callback

################################
# OAuth2 - Google
################################
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=http://localhost:8080/auth/google/callback
GOOGLE_LOCAL_REDIRECT_URI=http://localhost:8080/auth/google/callback

################################
# CORS+Cookie
################################
COOKIE_SECURE=false
COOKIE_SAMESITE=Lax

################################
# S3
################################
AWS_S3_BUCKET=your_bucket_name
AWS_REGION=ap-northeast-2
AWS_PRESIGN_EXP_SECONDS=120
AWS_S3_KEY_PREFIX=uploads
UPLOAD_PUBLIC_BASE_URL=https://your-cloudfront-url.net/
UPLOAD_MAX_BYTES=10485760

################################
# OpenAI
################################
APP_OPENAI_API_KEY=your_openai_api_key_here
